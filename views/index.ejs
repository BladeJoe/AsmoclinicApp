<!DOCTYPE html>
<html lang=<%= i18n.lang %>>
<%- include('partials/head'); %>
<%- include('partials/navbar'); %>
<main>
	<section class="hero">
		<div class="container">

			<div class="d-flex justify-content-end g-5 py-5 "
				style="background:center no-repeat ;background-image: url('/assets/images/hero-bg.jpg') ;margin: 30px 0;background-size: cover;min-height: 555px; ">

				<div class="col-lg-6 col-12 text-center text-lg-end p-3">
					<h1 class="lead my-5 fs-2 fw-bold p-4" style="background: rgba( 255, 255, 255, 0.55 );
						box-shadow: 0 8px 32px 0 rgba( 31, 38, 135, 0.37 );
						backdrop-filter: blur( 5px );
						-webkit-backdrop-filter: blur( 5px );
						border-radius: 10px;
						border: 1px solid rgba( 255, 255, 255, 0.18 );"> <%= i18n.hero %></h1>
					<a type="button" href="tel:+998555018866"
						class="btn btn-primary rounded-5 btn-lg px-4 me-md-2"><%= i18n.call %></a>
				</div>
			</div>
		</div>
	</section>
	<section class="container">
		<div class="d-flex justify-content-center flex-column align-items-center text-center">

			<h2 class="headline m-5"><%= i18n.levels %></h2>


		</div>
	</section>


	<section class="container">
		<div class="row-cols-1 row-cols-md-2 row-cols-lg-2 row-cols-xl-2 d-flex flex-row flex-wrap gap-5 align-items-center
				text-center justify-content-center">

			<div class="card rounded-4" style="width: 280px;height: 320px;">
				<div class="card-body d-flex flex-column align-items-center p-4">
					<img src="assets/images/consult.svg" alt="zoom" height="95" width="80">
					<h4 class="py-4"><%= i18n.card1Title %></h4>
					<p class="card-text"><%= i18n.card1Body %></p>
				</div>
			</div>

			<div class="card rounded-4" style="width: 280px;height: 320px;">
				<div class="card-body d-flex flex-column align-items-center  p-4">
					<img src="assets/images/lupa.svg" alt="zoom" height="95" width="80">
					<h4 class="py-4"><%= i18n.card2Title %></h4>
					<p class="card-text"><%= i18n.card2Body %></p>
				</div>
			</div>
			<div class="card rounded-4" style="width: 280px;height: 320px;">
				<div class="card-body d-flex flex-column align-items-center  p-4">
					<img src="assets/images/detail.svg" alt="zoom" height="95" width="80">

					<h4 class="py-4"><%= i18n.card3Title %> </h4>
					<p class="card-text"><%= i18n.card3Body %></p>
				</div>
			</div>
			<div class="card rounded-4" style="width: 280px;height: 320px;">
				<div class="card-body d-flex flex-column align-items-center  p-4">
					<img src="assets/images/frame.svg" alt="zoom" height="95" width="80">
					<h4 class="py-4"><%= i18n.card4Title %></h4>
					<p class="card-text"><%= i18n.card4Body %></p>
				</div>
			</div>





	</section>


	<section class="container my-5">
		<div class="swiper">

			<% for(var i=0; i<array.length; i++) { %>


			<a class="swiper-slide m-1 text-reset text-decoration-none " style="width: 350px;"
				href="team/<%= array[i].id %>">
				<img class="img" style="margin: auto 0;" src="/assets/images/avatar/team-<%= array[i].id %>.jpg"
					alt="img" width="150" height="">
				<div class="" style="display: flex;justify-content: space-between; flex-flow: column wrap;">

					<div class="name"><%= array[i].name  %></div>
					<div class="title"><%=array[i].position %></div>
				</div>
			</a>

			<% } %>

		</div>

	</section>

	<section class="container" loading="lazy">
		<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
			<div class="carousel-inner">
				<div class="carousel-item active">
					<img src="/assets/images/3Z8A9519.jpg" class="d-block w-100" alt="...">
				</div>
				<div class="carousel-item">
					<img src="/assets/images/3Z8A9522.jpg" class="d-block w-100" alt="...">
				</div>
				<div class="carousel-item">
					<img src="/assets/images/3Z8A9552.jpg" class="d-block w-100" alt="...">
				</div>
			</div>
			<button class="carousel-control-prev" name="previous" type="button"
				data-bs-target="#carouselExampleControls" data-bs-slide="prev">
				<span class="carousel-control-prev-icon" aria-hidden="true"></span>
				<span class="visually-hidden"><%= i18n.prev %></span>
			</button>
			<button class="carousel-control-next" name="next" type="button" data-bs-target="#carouselExampleControls"
				data-bs-slide="next">
				<span class="carousel-control-next-icon" aria-hidden="true"></span>
				<span class="visually-hidden"><%= i18n.next %></span>
			</button>
		</div>
		<a type="button" href="/gallery" class="btn btn-primary d-flex justify-content-center  rounded-5 btn-lg px-4  "
			style="width: 300px !important;margin: 20px auto;"><%= i18n.gallery %></a>
	</section>




	<section class="container my-5">
		<iframe title="map"
			src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1499.8377386679063!2d69.32236656417382!3d41.25062639760711!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x38ae5f37ad16b553%3A0xab6341606e52c63f!2sAsmo%20clinic!5e0!3m2!1sen!2spl!4v1714921765884!5m2!1sen!2spl"
			width="100%" height="600" style="border:0;" allowfullscreen="1" loading="lazy"
			referrerpolicy="no-referrer-when-downgrade"></iframe>
	</section>



</main>

<%- include('partials/footer'); %>
<div class="bar_wpp">
	<a href="https://www.t.me/murodpulatov" title="Telegram чат" target="_blank">
		<div class="icon_wpp bg-primary"> <i class="bi bi-telegram" aria-hidden="true"></i></div>
		<div class="txt_wpp">
			<%= i18n.telegramChat %>
		</div>
	</a>
</div>



</body>

</html>